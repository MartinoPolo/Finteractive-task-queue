{
	"name": "martinuv-prioritni-backend",
	"version": "1.0.0",
	"description": "Priority Queue Simulation Backend - Express with WebSocket",
	"author": "Martin",
	"license": "MIT",
	"main": "dist/index.js",
	"scripts": {
		"dev": "tsx watch --env-file=.env src/index.ts",
		"build": "tsc",
		"start": "node dist/index.js",
		"lint": "eslint src/**/*.ts",
		"test": "vitest",
		"test:run": "vitest run",
		"docker:build": "docker build -t task-queue-backend .",
		"docker:up": "docker compose -f ../docker-compose.yml up --build",
		"docker:down": "docker compose -f ../docker-compose.yml down"
	},
	"dependencies": {
		"cors": "^2.8.5",
		"express": "^4.18.2",
		"socket.io": "^4.8.1",
		"uuid": "^13.0.0",
		"zod": "^4.1.13"
	},
	"devDependencies": {
		"@types/cors": "^2.8.19",
		"@types/express": "^4.17.21",
		"@types/supertest": "^6.0.3",
		"supertest": "^7.1.4",
		"tsx": "^4.6.2",
		"typescript": "^5.3.2",
		"vitest": "^4.0.14"
	},
	"packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
