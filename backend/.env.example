PORT=3000                       # Server port
DEBUG=false                     # Enable debug logging (true/false)

AGING_FACTOR=60                 # Seconds before task gains +1 priority (prevents starvation)
PROGRESS_INCREMENT_MIN=10       # Min progress increment per tick
PROGRESS_INCREMENT_MAX=20       # Max progress increment per tick
PROCESSING_INTERVAL=5000        # Task processing interval in milliseconds

RATE_LIMIT_WINDOW_MS=60000      # API rate limit window in milliseconds
RATE_LIMIT_MAX_REQUESTS=100     # API rate limit max requests per window